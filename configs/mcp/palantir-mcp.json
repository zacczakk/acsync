{
  "description": "Palantir Foundry data access MCP server",
  "transport": "stdio",
  "command": "npx",
  "args": [
    "-y",
    "palantir-mcp",
    "--foundry-api-url",
    "${FOUNDRY_HOST}"
  ],
  "env_vars": [
    "FOUNDRY_TOKEN",
    "FOUNDRY_HOST"
  ],
  "env": {
    "FOUNDRY_TOKEN": "${FOUNDRY_TOKEN}"
  },
  "enabled": false,
  "disabled_for": [
    "codex"
  ]
}